[run]
source = codext
omit =
    codext/__info__.py
    codext/**/__init__.py
    tests/*

[report]
exclude_lines =
    pragma: no cover
    if.*?__name__.*?==.*?.__main__.:
    def main\(\)\:
    def __stdin_pipe\(\)\:
    except ImportError:
    except NameError:
    raise NotImplementedError
    if not PY3
    if PY3
    def encode\(self, input, final\=False\)\:
    def decode\(self, input, final\=False\)\:
